{
  "name": "chunches",
  "version": "1.0.0",
  "description": "Web app looking for places where you can buy the objects in a picture.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack watch --mode development",
    "dev:server": "nodemon --exec babel-node server/index.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack --mode production",
    "build:server": "babel ./server --out-dir ./build/server",
    "lint": "eslint --ignore-path .gitignore . --ext .js",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^6.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
